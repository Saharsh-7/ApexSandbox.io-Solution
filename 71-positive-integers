public List<Integer> positiveIntegers(List<Integer> numbers) {
    //code here

    List<Integer> positiveIntegersList = new List<Integer>();

    for (Integer i = 0; i < numbers.size(); i++) {
        if (numbers.get(i) > 0) {
            positiveIntegersList.add(numbers.get(i));
        }
    }

    return positiveIntegersList;
}
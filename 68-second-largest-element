}*/
    Integer max = numbers[0];
    Integer secondMax = numbers[0];

    for (Integer i = 0; i < numbers.size(); i++) {
        if (max <= numbers[i]) {
            secondMax = max;
            max = numbers[i];
        } else if (numbers[i] > secondMax && numbers[i] != max) {
            secondMax = numbers[i];
        }
    }

    return secondMax;
}
